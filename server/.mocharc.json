{
    "diff": true,
    "extension": [
        "ts"
    ],
    "package": "../package.json",
    "node-option": [
        "disable-warning=MODULE_TYPELESS_PACKAGE_JSON"
    ],
    "require": [
        "ts-node/register",
        "tsconfig-paths/register",
        "source-map-support/register",
        "reflect-metadata"
    ],
    "reporter": "spec",
    "timeout": 10000,
    "recursive": true,
    "exit": true,
    "spec": "app/**/*.spec.ts"
}