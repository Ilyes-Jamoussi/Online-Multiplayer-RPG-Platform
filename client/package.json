{
    "name": "client",
    "version": "1.0.0",
    "scripts": {
        "start": "ng serve --open",
        "build": "ng build",
        "deploy": "ng build",
        "format": "prettier --config ../.prettierrc.yaml --ignore-path ../.prettierignore --write \"src/**/*\"",
        "test": "ng test",
        "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
        "coverage": "ng test --code-coverage --source-map=true",
        "lint": "eslint --config eslint.config.mjs \"src/**/*.ts\"",
        "lint:fix": "eslint --config eslint.config.mjs \"src/**/*.ts\" --fix",
        "typecheck": "tsc -p tsconfig.app.json --noEmit",
        "knip": "knip",
        "check": "npm run typecheck && npm run knip",
        "check:unused-members": "tsx scripts/find-unused-members.ts",
        "check:unused-methods": "tsx scripts/find-unused-methods.ts",
        "check:duplicate-methods": "tsx scripts/find-duplicate-methods.ts",
        "check:unused-enum-members": "tsx scripts/find-unused-enum-members.ts",
        "check:unnecessary-files": "cd .. && ./scripts/check-unnecessary-files.sh",
        "check:unnecessary-files-xplatform": "node ../scripts/check-unnecessary-files.mjs",
        "check:quality": "cd .. && ./scripts/check-quality.sh",
        "check:quality-xplatform": "node ../scripts/check-quality.mjs",
        "generate-dto": "shx rm -rf src/app/dto && openapi-generator-cli generate -i http://localhost:3000/api/docs-json -g typescript-angular -c openapi-generator.config.json -o src/app/dto --global-property models --model-package . && shx rm -rf src/app/dto/@common"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "20.0.0",
        "@angular/cdk": "^20.2.2",
        "@angular/common": "20.0.0",
        "@angular/compiler": "20.0.0",
        "@angular/core": "20.0.0",
        "@angular/forms": "20.0.0",
        "@angular/material": "^20.2.2",
        "@angular/platform-browser": "20.0.0",
        "@angular/platform-browser-dynamic": "20.0.0",
        "@angular/router": "20.0.0",
        "@fortawesome/angular-fontawesome": "^3.0.0",
        "@fortawesome/free-solid-svg-icons": "^7.0.1",
        "html2canvas": "^1.4.1",
        "rxjs": "7.8.1",
        "socket.io-client": "4.8.1",
        "tslib": "2.4.0",
        "zone.js": "0.15.1"
    },
    "devDependencies": {
        "@angular-eslint/builder": "20.0.0",
        "@angular-eslint/eslint-plugin": "20.0.0",
        "@angular-eslint/eslint-plugin-template": "20.0.0",
        "@angular-eslint/schematics": "20.0.0",
        "@angular-eslint/template-parser": "20.0.0",
        "@angular/build": "^20.0.1",
        "@angular/cli": "20.0.1",
        "@angular/compiler-cli": "20.0.0",
        "@eslint/js": "9.32.0",
        "@openapitools/openapi-generator-cli": "^2.23.1",
        "@types/jasmine": "4.3.5",
        "@types/node": "22.10.2",
        "@typescript-eslint/eslint-plugin": "8.38.0",
        "@typescript-eslint/parser": "8.38.0",
        "baseline-browser-mapping": "^2.8.32",
        "eslint": "9.32.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "jasmine-core": "5.1.0",
        "karma": "6.4.2",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage": "2.2.1",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.1.0",
        "knip": "^5.64.1",
        "openapi-generator-cli": "^1.0.0",
        "prettier": "3.0.1",
        "shx": "^0.4.0",
        "ts-morph": "^27.0.2",
        "tsx": "^4.20.6",
        "typescript": "5.8.3",
        "typescript-eslint": "8.38.0"
    }
}
